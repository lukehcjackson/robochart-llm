<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
     <extension
           point="org.eclipse.ui.startup">
           <startup
            class="circus.robocalc.robochart.generator.llm.Startup">
      </startup>
     </extension> 
     <extension
     		point="org.eclipse.ui.menus">
     		<menuContribution
     			allPopups="false"
     			locationURI="popup:robotool.contextMenu">
     			<command
     				commandId="circus.robocalc.robochart.generator.llm.commands.gen"
     				label="LLM RoboChart Generator"
     				style="push"
     				tooltip="Generate a RoboChart model using an LLM">
     			</command>
     		</menuContribution>
     </extension>
     <extension
     		point="org.eclipse.ui.handlers">
     		<handler
     			class="circus.robocalc.robochart.generator.llm.Handler"
     			commandId="circus.robocalc.robochart.generator.llm.commands.gen">
     		</handler>
     </extension>
</plugin>
